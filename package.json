{
  "name": "access-watch-ui",
  "version": "1.8.8",
  "description": "The Access Watch User Interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/access-watch/access-watch-ui.git"
  },
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "author": {
    "name": "Access Watch Technologies GmbH",
    "email": "team@access.watch"
  },
  "contributors": [
    "Jean Duthon <jean@access.watch>",
    "Fran√ßois Hodierne <francois@access.watch>"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development webpack-dev-server --hot",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint './src/**/*.{js,jsx}' './__tests__/**/*.js'",
    "build": "NODE_ENV=production webpack",
    "build:staging": "NODE_ENV=staging webpack",
    "clean": "rm -rf ./dist",
    "prettier": "prettier --write '**/*'",
    "prettier:watch": "prettier --watch '**/*'",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "access-watch-sdk": "^0.1.5",
    "access-watch-ui-components": "^1.0.3",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-jest": "^22.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.24.1",
    "blacklist": "^1.1.4",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "core-js": "^2.4.1",
    "country-data": "0.0.31",
    "css-loader": "^0.28.1",
    "date-fns": "^1.15.1",
    "director": "^1.2.8",
    "elemental": "git+https://github.com/access-watch/elemental.git#build",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.7",
    "hastscript": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-cli": "^22.4.0",
    "json-loader": "^0.5.4",
    "lerna": "^2.9.0",
    "node-sass": "^4.5.2",
    "postcss-loader": "^2.0.3",
    "prettier": "1.10.2",
    "prop-types": "^15.6.0",
    "qs": "^6.3.0",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-transition-group": "^1.2.1",
    "rxjs": "^5.5.6",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.20.2",
    "svg-react-loader": "^0.4.5",
    "timeago.js": "^3.0.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.4.5",
    "whatwg-fetch": "^2.0.1"
  },
  "jest": {
    "bail": true,
    "resetModules": true,
    "timers": "fake",
    "globals": {
      "__DEV__": true
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      ".*": "./jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "^!raw-loader!(.+)$": "<rootDir>/jest/empty.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jest/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/packages"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/src/rx",
      "<rootDir>/node_modules/rxjs"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "react-transition-group"
    ]
  }
}
